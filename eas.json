{
  "cli": {
    "version": ">= 14.3.0",
    "appVersionSource": "remote"
  },
  "build": {
   "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"

    },
    "development:simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios":{
        "simulator":true
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"

    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
    
    "channel": "production"

    }
  },
  "submit": {
    "production": {}
  }
}
